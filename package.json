{
	"name": "admin-dashboard",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "pnpm sync-types && vite dev",
		"build": "pnpm sync-types && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"codegen:component": "tsx scripts/codegen-component.ts",
		"codegen:route": "tsx scripts/codegen-route.ts",
		"codegen:schema": "npx graphql-codegen --config codegen.ts",
		"codegen:schema:check": "npx graphql-codegen --config codegen.ts --check",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"fetch:types": "VITE_API_BASE_URL=http://localhost:4000/api/v1 tsx src/scripts/fetch-types.ts",
		"fetch:schema": "VITE_API_BASE_URL=http://localhost:4000/api/v1 tsx src/scripts/fetch-schema.ts",
		"sync-types": "cd ../backend && pnpm extract-types && cd ../admin-dashboard && pnpm fetch:types && pnpm fetch:schema",
		"test:unit": "vitest",
		"typecheck": "tsc",
		"test": "pnpm test:unit -- --run && pnpm test:e2e",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"test:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"test:e2e": "playwright test",
		"verify": "pnpm lint && pnpm typecheck && pnpm check"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^3.0.0",
		"@eslint/js": "^9.16.0",
		"@graphql-codegen/cli": "^5.0.3",
		"@graphql-codegen/client-preset": "^4.5.1",
		"@graphql-codegen/typed-document-node": "^5.0.12",
		"@graphql-codegen/typescript": "^4.1.2",
		"@graphql-codegen/typescript-operations": "^4.4.0",
		"@graphql-codegen/typescript-resolvers": "^4.4.1",
		"@histoire/plugin-svelte": "^0.17.17",
		"@iconify/svelte": "^4.0.2",
		"@playwright/test": "^1.49.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@testing-library/user-event": "^14.5.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^9.6.1",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.10.1",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"autoprefixer": "^10.4.20",
		"cssnano": "^7.0.6",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.13.0",
		"histoire": "^0.17.17",
		"postcss": "^8.4.49",
		"postcss-import": "^16.1.0",
		"postcss-nested": "^7.0.2",
		"postcss-preset-env": "^10.1.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^4.2.19",
		"svelte-check": "^4.1.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.2",
		"typescript-eslint": "^8.17.0",
		"unplugin-icons": "^0.20.2",
		"vite": "^5.4.11",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"@editorjs/editorjs": "^2.30.7",
		"@editorjs/embed": "^2.7.6",
		"@editorjs/header": "^2.8.8",
		"@editorjs/image": "^2.10.1",
		"@editorjs/list": "^2.0.2",
		"@editorjs/quote": "^2.7.6",
		"@fontsource/almarai": "^5.1.0",
		"@grassrootsfoundation/trusty-ui": "file:../trusty-ui",
		"@sveltejs/kit": "^2.9.0",
		"editorjs-drag-drop": "^1.1.16",
		"jsonwebtoken": "^9.0.2",
		"node-fetch": "^3.3.2",
		"trusty-css": "^1.0.1",
		"typescript": "^5.7.2"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
